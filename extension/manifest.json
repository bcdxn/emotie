{
  "manifest_version": 2,
  
  "name": "Emotie",
  "short_name": "Emotie",
  "description": "Quick access to plain text emoticons",
  "version": "0.4.0.1",
  "browser_action": {
    "default_icon":"img/icon128.png",
    "default_popup": "pop-up.html"
  },
  "icons": {
    "128": "img/icon128.png"
  },
  "permissions": [
    "storage"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}